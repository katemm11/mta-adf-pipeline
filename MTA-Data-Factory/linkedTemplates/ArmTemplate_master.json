{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MTA-Data-Factory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"MTALinkedService_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'MTALinkedService'"
		},
		"MTASqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MTASqlDatabase'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mtastorage2.dfs.core.windows.net/"
		},
		"MTALinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1001-204325-tmpmxzov"
		},
		"MTASqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mta-server-2.database.windows.net"
		},
		"MTASqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mta_database"
		},
		"MTASqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mta_server_admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MTA-Data-Factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"MTALinkedService_accessToken": {
						"value": "[parameters('MTALinkedService_accessToken')]"
					},
					"MTASqlDatabase_password": {
						"value": "[parameters('MTASqlDatabase_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"MTALinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('MTALinkedService_properties_typeProperties_existingClusterId')]"
					},
					"MTASqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('MTASqlDatabase_properties_typeProperties_server')]"
					},
					"MTASqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('MTASqlDatabase_properties_typeProperties_database')]"
					},
					"MTASqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('MTASqlDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}